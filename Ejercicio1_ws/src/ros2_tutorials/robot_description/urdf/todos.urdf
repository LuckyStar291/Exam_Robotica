  </joint>

  <link name="r2_link4"/>
  <link name="r2_link4_x_axis">
    <visual><origin xyz="0 0 0.25"/><geometry><cylinder length="0.5" radius="0.05"/></geometry><material name="red"/></visual>
  </link>
  <joint name="r2_q4" type="revolute">
    <origin xyz="0 0 0"/><parent link="r2_link3_passive"/><child link="r2_link4"/><axis xyz="0 0 1"/>
    <limit effort="80" velocity="2.0" lower="-1.57" upper="1.57"/>
  </joint>
  <joint name="r2_q4_passive" type="fixed">
    <origin xyz="2 0 0"/><parent link="r2_link4"/><child link="r2_link4_passive"/>
  </joint>
  <link name="r2_link4_passive">
