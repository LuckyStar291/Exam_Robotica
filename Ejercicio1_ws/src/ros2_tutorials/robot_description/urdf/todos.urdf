  </link>
  <joint name="r2_q4_x" type="fixed">
    <origin xyz="0 0 0" rpy="0 1.571 0"/><parent link="r2_link4_passive"/><child link="r2_link4_x_axis"/>
  </joint>

  <link name="r2_link5"/>
  <link name="r2_link5_x_axis">
    <visual><origin xyz="0 0 0.25"/><geometry><cylinder length="0.5" radius="0.05"/></geometry><material name="red"/></visual>
  </link>
  <joint name="r2_q5" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 -0.8"/><parent link="r2_link4_passive"/><child link="r2_link5"/><axis xyz="0 0 1"/>
    <limit effort="80" velocity="2.0" lower="-1.57" upper="1.57"/>
  </joint>
  <joint name="r2_q5_passive" type="fixed">
    <origin xyz="1 0 0"/><parent link="r2_link5"/><child link="r2_link5_passive"/>
  </joint>
  <link name="r2_link5_passive">
    <visual><origin xyz="0 0 0.25"/><geometry><cylinder length="0.5" radius="0.05"/></geometry><material name="blue"/></visual>
  </link>
  <joint name="r2_q5_x" type="fixed">
    <origin xyz="0 0 0" rpy="0 1.571 0"/><parent link="r2_link5_passive"/><child link="r2_link5_x_axis"/>
  </joint>

</robot>